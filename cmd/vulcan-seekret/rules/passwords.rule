password:
  match: .*pass(word)?\s*=\s*.*[^ \'";,\*].*
  unmatch: 
    - =.*pass(word).*
login_in_uri:
  match: ([\w+]{1,24})(://)([^$<]{1})([^\s";]{1,}):([^$<]{1})([^\s";]{1,})@[-a-zA-Z0-9@:%._\+~#=]{1,256}\.[a-zA-Z0-9()]{1,24}([^\s]+)
ssh_password:
  match: sshpass -p.*['|\\\"]
unix_passwd:
  match: .*\w+:[^:]:\d+:\d+:[\w ]+:[\w\/]+:[\w\/]+.*
